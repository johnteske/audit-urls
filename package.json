    "format": "prettier src/*.ts test.js --write",
    "test": "npm run build && tape test.js"
  },
  "keywords": [],
  "author": "John Teske",
  "license": "MIT",
  "dependencies": {
    "got": "^11.1.3"
  },
  "devDependencies": {
    "prettier": "^2.0.5",
    "tape": "^5.0.0",
    "tape-promise": "^4.0.0",
    "typescript": "^3.9.2"
  }
}

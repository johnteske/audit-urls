{
  "name": "audit-urls",
  "version": "0.0.1",
  "description": "audit url statuses, including https availability",
  "main": "dist/index.js",
  "bin": {
    "check-urls": "./dist/cli.js"
  },
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "tsc src/*.ts --declaration --outDir dist && chmod +x dist/cli.js",
